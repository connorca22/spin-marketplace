<div class="navbar"> 
<%= link_to 'Home', root_path, class: "button button-primary" %>
<%= link_to 'Listings', listings_path, class: "button button-primary" %>

<span style="float: right">
    <% if user_signed_in? %>
    <%= link_to 'Profile', profile_path, class: "button button-primary" %>
    <%= link_to 'Sign Out', destroy_user_session_path, method: :delete, class: "button button-primary" %>
    <% else %>
    <%= link_to 'Sign Up', new_user_registration_path, class: "button button-primary" %>
    <%= link_to 'Sign In', new_user_session_path, class: "button button-primary" %>
    <% end %>
</span>
</div>
